{
  "title": "Cambios en terminal",
  "rules": [
    {
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "com.apple.Terminal",
                "com.googlecode.iterm2"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "description": "terminal right_command to right_control",
          "from": {
            "key_code": "right_command"
          },
          "to": {
            "key_code": "right_control"
          },
          "type": "basic"
        }
      ]
    },
    {
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": ["com.apple.Terminal"],
              "type": "frontmost_application_if"
            }
          ],
          "description": "terminal right_option to right_command",
          "from": {
            "key_code": "right_option"
          },
          "to": {
            "key_code": "right_command"
          },
          "type": "basic"
        }
      ]
    }
  ]
}
