{
    "title": "Arreglar Ctrl",
    "rules": [
        {
            "manipulators": [
                {
                    "description": "Ctrl+Tab to Cmd+Tab",
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "tab",
                        "modifiers": ["right_command"]
                    }],
                    "type": "basic"
                }
            ]
        }
    ]
}